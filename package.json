{
  "name": "template-pug",
  "version": "1.0.0",
  "description": "template-pug",
  "author": "Segawa Akira",
  "license": "MIT",
  "scripts": {
    "build": "webpack --mode production --progress ",
    "start": "concurrently \"npm run imagemin:watch\" \"webpack-dev-server --mode development --inline --open\"",
    "imagemin:build": "node imagemin.js",
    "imagemin:watch": "onchange './src/images' -e '**/*.DS_Store' -- npm run imagemin:build"
  },
  "devDependencies": {
    "autoprefixer": "^8.2.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "concurrently": "^4.1.1",
    "css-loader": "^3.0.0",
    "css-mqpacker": "^6.0.2",
    "extract-text-webpack-plugin": "next",
    "file-loader": "^1.1.11",
    "glob": "^7.1.2",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.1.0",
    "imagemin": "^7.0.0",
    "imagemin-gifsicle": "^6.0.1",
    "imagemin-keep-folder": "^5.3.2",
    "imagemin-mozjpeg": "^8.0.0",
    "imagemin-pngquant": "^8.0.0",
    "imagemin-svgo": "^7.0.0",
    "js-yaml": "^3.13.1",
    "js-yaml-loader": "^0.1.0",
    "node-sass": "^4.8.3",
    "onchange": "^6.0.0",
    "portfinder": "^1.0.13",
    "postcss-loader": "^2.1.3",
    "pug": "^2.0.3",
    "pug-loader": "^2.4.0",
    "read-config": "^2.0.0",
    "reset.css": "^2.0.2",
    "sass-loader": "^6.0.7",
    "webpack": "^4.4.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.1",
    "webpack-route-data-mapper": "^2.1.0"
  },
  "babel": {
    "presets": [
      "env"
    ],
    "plugins": [
      "transform-runtime"
    ]
  },
  "browserslist": [
    "ie >= 11",
    "ios >= 9",
    "android >= 4.4"
  ],
  "dependencies": {
    "lodash": "^4.17.14"
  }
}
